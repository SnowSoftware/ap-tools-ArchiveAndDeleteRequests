    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "args": [
                "-APDatabaseServer '192.168.168.168,51429'",
                "-DeleteBeforeThisDate 2023-06-01",
                "-RequestParameterBatchSize 1000",
                "-BatchesToRun 3",
                "-InformationAction continue"
            ]
